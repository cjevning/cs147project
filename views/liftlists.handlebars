    <div id="tableDiv" name="Lift">
      <div class="panel-group" id="accordion">
        <div class="panel panel-default accHeight">
          <div class="panel-heading override">
            <table class="override" data-toggle="collapse" data-parent="#accordion" href="#collapseCreate">
              <tr class="override">
                <td class="rowName override" id="rowFirst">
                  <div class="override itemName">
                    Create a Workout
                  </div>
                </td>
                <td class="dropDown">                  
                </td>
              </tr>
            </table>
          </div>
        </div>
        <div id="collapseCreate" class="panel-collapse collapse">
          <div class="panel-body">
            <div class="expandedList">
                <div id="formDiv">
                  <p class="pcenter">Title for Workout:</p>
                  <form id="new-list-form" action="user_login" role="form">
                    <div class="form-group">
                      <input type="text" class="form-control input" id="title" placeholder="Workout"><button id="newListSubmitButton" type="button" class="btn btn-default login buttonH buttonH2" onclick="createList()">Create</button></input>
                    </div>
                  </form>
                </div>
            </div>
          </div>
        </div>


      <script type="text/javascript">
        var count = 0;
      </script>
      
      {{#each lists}}
      <div class="panel panel-default accHeight">
        <div class="panel-heading override">
          <script>
            document.write("<table class=\"override\" data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#collapse" + count + "\">");
          </script>
          <!--<table class="override" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= count %>">-->
            <tr class="override">
              <script>
                document.write("<td class=\"rowName override\" id=\"row" + count + "\">");
              </script>
              <!--<td class="rowName override" id="row<%= count %>">-->
                <div class="override itemName">
                  {{name}}
                </div>
              </td>
              <td class="dropDown">                  
              </td>
            </tr>
          </table>
        </div>
      </div>

      <script>
        document.write("<div id=\"collapse" + count + "\" class=\"panel-collapse collapse\">");
      </script>
      <!--<div id="collapse<%= count %>" class="panel-collapse collapse">-->
        <div class="panel-body">
          <div class="expandedList">
            <div class="storeStatsContainer liftStats storeRow">
                <script type="text/javascript">
                  var last = '{{lastDone}}';
                  var sp = last.split(" ");
                  var year = sp[3];
                  var x = "Never";
                  if (year != '1992') {
                    x = sp[1] + " " + sp[2] + " " + sp[3];
                  }
                  document.write("<p class=\"storeInfo\">Last Done: " + x + "</p>");
                </script>
            </div>
            <div class="buttonsContainer">
              <table class="liftButtons">
                <tr class="innerLiftButton">
                  <td class="innerLiftButton doneButtonDiv">
                    <a class="invis" href="viewList/{{_id}}">
                    <div class="innerLiftButton viewButton">
                    </div>
                    <div class="buttonUnder">Go to</div>
                  </a>
                  </td>
                </tr>
              </table>
            </div>
          </div>
        </div>
      </div>
      <script type="text/javascript">
        count += 1;
      </script>
      {{/each}}
